{
    "env": {
        "node": true,
        "commonjs": false,
        "es2021": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jsdoc/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/extensions": [
            "error",
            "ignorePackages",
            {
                "js": "true"
            }
        ]
    }
}